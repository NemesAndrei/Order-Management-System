<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="315.0" prefWidth="295.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Presentation.Controller">
    <children>
        <Button fx:id="showClientMenu" layoutX="72.0" layoutY="114.0" mnemonicParsing="false" onAction="#showClient"
                prefHeight="25.0" prefWidth="151.0" text="Client Menu"/>
        <Button fx:id="showOrderMenu" layoutX="72.0" layoutY="145.0" mnemonicParsing="false" onAction="#showOrder"
                prefHeight="25.0" prefWidth="151.0" text="Order Menu"/>
        <Button fx:id="showProductMenu" layoutX="72.0" layoutY="177.0" mnemonicParsing="false" onAction="#showProduct"
                prefHeight="25.0" prefWidth="151.0" text="Product Menu"/>
    </children>
</AnchorPane>
